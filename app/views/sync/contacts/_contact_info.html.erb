<div class="row">
    <div class="col s12">
      <div class="card ">
        <div class="card-content row">

          <div class="card-image s12 l3 col">
          <%= image_tag contact.primary_photo, class: "align-right circle"%>
          </div>
          <div class=" s12 l9 col">
          <div class="card-title">
            <%= link_to contact.contact_info['fullName'] , contact %>
          </div>
            <label> Email: </label><%= contact.email%><br>
            <% if contact.demographics %>
              <label> Gender: </label><%= contact.demographics['gender'] %><br>
              <label> Location: </label><%= (contact.demographics['locationDeduced'] && contact.demographics['locationDeduced']['deducedLocation']) || contact.demographics['locationGeneral']%><br>
              <%end %>
          </ul>
        </div>
      </div>
    </div>
  </div>
