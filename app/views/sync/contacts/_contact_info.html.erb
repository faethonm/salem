<div class="row">
    <div class="col s12">
      <div class="card">
        <div class="card-content ">
          <span class="card-title">
            <%= link_to contact.contact_info['fullName'] , contact %></span>
          <ul class="collection">
            <li class="collection-item ">
              <%= contact.contact_info['fullName'] %>
            </li>
            <li class="collection-item"><%= contact.email%></li>
            <% if contact.demographics %>
              <li class="collection-item"><%= contact.demographics['gender'] %></li>
              <li class="collection-item"><%= (contact.demographics['locationDeduced'] && contact.demographics['locationDeduced']['deducedLocation']) || contact.demographics['locationGeneral']%></li>
              <%end %>
          </ul>
        </div>
      </div>
    </div>
  </div>
