
  <% if @contact.contact_info %>
  <div class="row">
    <div id="test1" class="col s8 offset-s2">
      <%= render 'shared/contact_info' %>
    </div>
    </div>

    <div id="test2" class="col s8 offset-s2">
      <div class="card">
        <div class="card-content ">
          <span class="card-title pink-text text-darken-4">Social Presence</span>
          <ul class="collection">
            <% @contact.social_profiles.each do |social_profile| %>
              <%= render partial: 'shared/social_profile' ,locals: {social_profile: social_profile} %>
            <% end %>
          </ul>
        </div>
      </div>
    </div>
    <% else %>
      <%= render 'shared/no_social_information' %>
    <% end %>
</div>
