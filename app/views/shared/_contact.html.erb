<div class="row">
  <% if @contact.contact_info %>
    <div id="test1" class="col s12">
      <%= sync partial: 'contact_info', resource: @contact %>
    </div>
    <div id="test2" class="col s12">
      <div class="card">
        <div class="card-content ">
          <span class="card-title">Social Information</span>
          <ul class="collection">
            <% @contact.social_profiles.each do |social_profile| %>
              <%= render partial:'social_profile' ,locals: {social_profile: social_profile} %>
            <% end %>
          </ul>
        </div>
      </div>
    </div>
    <% else %>
      <%= render 'shared/no_social_information' %>
    <% end %>
</div>
