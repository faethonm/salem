<div class="row">
  <div class="col s12">
    <div class="card">
      <div class="card-image">
         <% image_tag(@primary_photo, alt: 'no image') %>
        <span class="card-title">Card Title</span>
      </div>
      <div class="card-content ">
        <ul class="collection">
          <li class="collection-item ">
            <%= @contact_info.full_name %>
          </li>
          <li class="collection-item"><%= @social_info.email%></li>
          <% if @demographics %>
            <li class="collection-item"><%= @demographics.gender %></li>
            <li class="collection-item"><%= (@demographics.location_deduced && @demographics.location_deduced.deduced_location) || @demographics.location_general%></li>
            <%end %>
        </ul>
      </div>
    </div>
  </div>
</div>
